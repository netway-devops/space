{
    "name": "billing.netway.co.th Main",
    "dockerComposeFile": "../../../../laradock-multisite/docker-compose-billing-netway-main.yml",
    "remoteUser": "laradock",
    "runServices": [
        "apache2",
        "php-fpm",
        "mysql",
        "redis",
        "phpmyadmin",
        "workspace",
        "docker-in-docker",
        "mailhog"
