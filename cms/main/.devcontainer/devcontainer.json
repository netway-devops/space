{
  "name": "CMS Main",
  "dockerComposeFile": "../../../../laradock-multisite/docker-compose-cms-main.yml",
  "remoteUser": "laradock",
  "runServices": [
    "apache2",
    "php-fpm",
    "mysql",
    "mariadb",
    "phpmyadmin",
    "workspace",
    "docker-in-docker",
    "mailhog"
  ],
  "service": "workspace",
  "workspaceFolder": "/home/laradock/rvsitebuildercms",
  "shutdownAction": "stopCompose",
  "preCreateCommand": "cd ../../../../laradock-multisite/; ./local-ssl-certs-gen.sh",
  "postCreateCommand": "unlink /home/laradock/main; ln -s /home/laradock/rvsitebuildercms /home/laradock/main; npm install -g npm@6;",
}